

<div id="chart"></div>

<div class="pure-u-1-4" id = "pie"></div>
<div class="pure-u-1-4" id = "pie2"></div>
<div class="pure-u-1-4" id = "pie3"></div>
<div class="pure-u-1-4" id = "pie4"></div>

<script type="text/javascript">

var chart = c3.generate({
	bindto: '#chart',
    data: {
        x: 'x',
        xFormat: '%Y', // 'xFormat' can be used as custom format of 'x'
        columns: [
            ['x', '2010', '2011', '2012', '2013'],
      		<%=mean.reverse.unshift("Average Score Per Year")%>
        ]
    },
    axis: {
        x: {
            type: 'timeseries',
            tick: {
                format: '%Y'
            }
        }
    }
});

var pie = c3.generate({
	bindto:"#pie",
    data: {
        columns: [
            ['First', <%=first[0]%>],
            ['Second', <%=second[0]%>],
            ['Third', <%=third[0]%>],
            ['Fourth', <%=fourth[0]%>],
            ['Fifth', <%=sixth[0]%>],
            ['Sixth', <%=sixth[0]%>]
        ],
        title: "2013 Breakdown",
        type : 'pie',
    }
});

var pie2 = c3.generate({
	bindto:"#pie2",
    data: {
        columns: [
            ['First', <%=first[1]%>],
            ['Second', <%=second[1]%>],
            ['Third', <%=third[1]%>],
            ['Fourth', <%=fourth[1]%>],
            ['Fifth', <%=sixth[1]%>],
            ['Sixth', <%=sixth[1]%>]
        ],
        title: "2013 Breakdown",
        type : 'pie',
    }
});

var pie3 = c3.generate({
	bindto:"#pie3",
    data: {
        columns: [
            ['First', <%=first[2]%>],
            ['Second', <%=second[2]%>],
            ['Third', <%=third[2]%>],
            ['Fourth', <%=fourth[2]%>],
            ['Fifth', <%=sixth[2]%>],
            ['Sixth', <%=sixth[2]%>]
        ],
        title: "2013 Breakdown",
        type : 'pie',
    }
});

var pie4 = c3.generate({
	bindto:"#pie4",
    data: {
        columns: [
            ['First', <%=first[3]%>],
            ['Second', <%=second[3]%>],
            ['Third', <%=third[3]%>],
            ['Fourth', <%=fourth[3]%>],
            ['Fifth', <%=sixth[3]%>],
            ['Sixth', <%=sixth[3]%>]
        ],
        title: "2013 Breakdown",
        type : 'pie',
    }
});

d3.select('#pie svg').append('text')
    .attr('x', d3.select('#chart svg').node().getBoundingClientRect().width / 2)
    .attr('y', 16)
    .attr('text-anchor', 'middle')
    .style('font-size', '1.4em')
    .text('2013');

</script>