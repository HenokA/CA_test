<div class = "pure-g-1-1" >
<div class = "splash-container pure-u-1-2">
	<div class = "splash">
		<h1 class="splash-head"> San Francisco Standardized <br> Test Score Graph </h1>
		<p class = "splash-subhead"> A website that graphs SF's standardized test scores from 2010-2013. </p>
	</div>
</div>


<div class = " choices pure-u-1-3">
<h1 class = "splash-head"> Select Information Here </h1>
	<form action= "/home" method = "post" class="pure-form pure-form-stacked">
	    <fieldset>
	         <label for="city">School</label>
	        <select id="school" name="school">
	        	<% names.each do |name| %>
	        	<option> <%=name%> </option>
	            <%end%>
	        </select>
	        </label>

	         <label for="test">Test</label>
	        <select id="test" name="test">
	            <option>7</option>
	            <option>9</option>
	            <option>11</option>
	        </select>

	         <label for="grade">Grade</label>
	        <select id="grade" name="grade">
	            <option>5</option>
	            <option>6</option>
	            <option>7</option>
	            <option>8</option>
	            <option>9</option>
	            <option>10</option>
	            <option>11</option>

	        </select>
	        <button name = "button1" type="submit" class="pure-button pure-button-primary"> Search </button>
	    </fieldset>
	</form>
</div>

</div>

<div class = "pure-u-1-3">
<img src = "http://i.imgur.com/ppvvxZe.png"/>
</div>